{
	"compilerOptions": {
		"declaration": true,
		"lib": [
			"es6"
		],
		"module": "commonjs",
		"outDir": "../out",
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"removeComments": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"strict": true,
		"target": "es6"
	}
}